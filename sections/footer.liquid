{% if template != 'index' %}
  <footer class="w-full relative">
    <div class="w-full pt-[120px] lg:pt-12 pb-6 lg:pb-10 relative after:top-4 after:bottom-4 after:w-[1px] after:bg-bone">
      <div class="px-4 w-full grid grid-cols-10 lg:grid-cols-20 gap-2 lg:gap-3"></div>
    </div>
  </footer>

  <footer
    id="footer"
    class="relative bg-brown text-cream px-5 lg:px-9 flex flex-col gap-10 lg:gap-5 pt-9"
  >
    <div class="w-full flex flex-col md:grid md:grid-cols-2 lg:grid-cols-12 gap-2 md:gap-x-5 lg:gap-10">
      <div class="lg:col-span-4">
        {% render 'newsletter' %}
      </div>
      <div class="lg:col-span-6 grid grid-cols-2 lg:grid-cols-3 gap-y-6 lg:gap-y-5 mt-8 lg:mt-0">
        {% for link in section.settings.menu.links %}
          <div class="flex flex-col gap-y-3.5 w-full">
            <p class="p3-futura lg:p4-futura">{{ link.title }}</p>
            {% if link.links %}
              <ul class="flex flex-col gap-y-1.5">
                {% for childLink in link.links %}
                  <li class="contents">
                    <a
                      href="{{ childLink.url }}"
                      class="p3-souvenir"
                      {% if childLink.url contains '://' %}
                        target="_blank"
                      {% endif %}
                    >
                      {{- childLink.title -}}
                    </a>
                  </li>
                {% endfor %}
              </ul>
            {% endif %}
          </div>
        {% endfor %}
        {% if section.settings.contact != blank %}
          <div class="flex flex-col gap-y-3.5 w-full col-span-2 lg:col-span-1">
            <p class="p3-futura lg:p4-futura">Contact</p>
            <div class="flex flex-col gap-y-1.5 p3-souvenir">
              {{ section.settings.contact }}
            </div>
            <div class="flex lg:contents items-center justify-between">
              <div class="flex items-center gap-x-4">
                {% if section.settings.facebook != blank %}
                  <a
                    target="_blank"
                    href="{{ section.settings.facebook }}"
                    aria-label="BlakeBrown Facebook"
                    class="w-max h-max block"
                  >
                    <svg
                      class="w-2.5 h-5 relative fill-current"
                      width="10"
                      height="21"
                      viewBox="0 0 10 21"
                    >
                      <path d="M2.89996 20.4367H6.79243V10.3072H9.50922L9.79519 6.91415H6.79243V4.9824C6.79243 4.15685 6.94336 3.86792 7.69008 3.86792H9.79519V0.351074H7.10222C4.20273 0.351074 2.89996 1.6719 2.89996 4.21459V6.91415H0.858398V10.3485H2.88406L2.89996 20.4367Z" />
